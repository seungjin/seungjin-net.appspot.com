<?xml version="1.0" encoding="UTF-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://kenai.com/projects/nbappengine/downloads/download/schema/appengine-web.xsd appengine-web.xsd">
    <application>seungjin-net</application>
    <version>1</version>
    <static-files>
        <include path="/**.png"/>
        <include path="/**.jpg"/>
        <include path="/**.html"/>
        <include path="/**.ico"/>
        <include path="/**.xsl"/>
        <include path="/robots.txt"/>
        <include path="/images/*"/>
        <include path="/styles/*"/>
        <include path="/scripts/*"/>
    </static-files>
    <ssl-enabled>true</ssl-enabled>
    <sessions-enabled>true</sessions-enabled>
    <inbound-services>
        <service>mail</service>
    </inbound-services>
</appengine-web-app>
